<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <!-- only backup our sharedprefs-->
    <cloud-backup>
        <include
            domain="sharedpref"
            path="Main.xml" />
    </cloud-backup>
    <device-transfer>
        <include
            domain="sharedpref"
            path="Main.xml" />
    </device-transfer>
</data-extraction-rules>
